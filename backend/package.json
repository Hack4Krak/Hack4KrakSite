{
  "name": "hack4krak-backend",
  "private": true,
  "scripts": {
    "install": "cargo fetch",
    "build": "cargo build",
    "dev": "cargo run",
    "lint": "cargo fmt -- --check && cargo clippy",
    "lint:fix": "cargo fmt && cargo clippy --fix --allow-staged --allow-dirty",
    "test": "cargo test",
    "test:full": "cargo test --features=full-test-suite",
    "openapi-check": "cargo run -p openapi-check"
  }
}
